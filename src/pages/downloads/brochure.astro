---
// src/pages/downloads/brochure.astro
import Layout from "@/layouts/Layout.astro";
import seoData from "@/data/js/seo/index";
import PdfViewer from "@/components/modals/PdfViewer.jsx";
import Loader from "@/components/other/Loader.astro";

const { meta, faqSchema, gtm, pageLink, schema } = seoData;
---

<Layout
  seo={{
    title: `${meta.title} - Indivirtus`,
    description: meta.description,
    keywords: meta.keywords,
    canonical: meta.canonicalUrl,
    url: pageLink,
    schema: schema,
    faqSchema: faqSchema,
    ogTags: meta.ogTags,
    twitterCard: meta.twitterCard,
  }}
  gtm={gtm}
>
  <!-- <PdfViewer client:load /> -->
  <section class="maxwidth">
    <div class="relative">
      <Loader id="pdf-loader" />
      <PdfViewer file="/pdfFiles/IES-brochure.pdf" client:load />
    </div>
  </section>
</Layout>

<style></style>

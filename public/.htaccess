<IfModule mod_rewrite.c>
     RewriteEngine On
     RewriteBase /

     # Redirect IES-brochure.pdf â†’ /downloads/brochure/  (301), preserving query string.
     RewriteCond %{REQUEST_URI} ^/downloads/IES-brochure\.pdf$ [NC]
     RewriteRule ^downloads/IES-brochure\.pdf$ /downloads/brochure/ [R=301,L]
     RewriteCond %{REQUEST_FILENAME} !-f
     RewriteCond %{REQUEST_FILENAME} !-d
     RewriteCond %{REQUEST_FILENAME} !-l
     RewriteRule ^(.*)$ /404.html [L]
     </IfModule>
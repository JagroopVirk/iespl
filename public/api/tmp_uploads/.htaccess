# Disable directory listing

Options -Indexes

# Deny access to all files

<FilesMatch ".\*">
Order Allow,Deny
Deny from all
</FilesMatch>

# If using Apache 2.4+

<IfModule mod_authz_core.c>
    Require all denied
</IfModule>

# Disable PHP execution in this folder

<FilesMatch "\.(php|php5|php7|phtml)$">
Order Allow,Deny
Deny from all
</FilesMatch>
